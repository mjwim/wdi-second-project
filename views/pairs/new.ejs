<h2>Pairs New</h2>

<form class="form-group" method="POST" action="/pairs">
  <div>
    <label for="wine">Wine</label>
    <input class="form-control" type="text" name="wine" id="wine">
  </div>

  <div>
    <label for="cheese">Cheese</label>
    <input class="form-control" type="text" name="cheese" id="cheese">
  </div>

  <div>
    <label for="createdBy">Created by</label>
    <input class="form-control" type="text" name="createdBy" id="createdBy">
  </div>

  <div>
    <label for="description">Description</label>
    <textarea class="form-control" name="description"></textarea>
  </div>

  <button class="btn btn-primary">Create</button>
</form>
